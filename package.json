{
  "name": "pustaka-author-dashboard",
  "version": "1.0.0",
  "description": "API for authors managing and getting stats for their books and revenue from Pustaka Digital Media Pvt. Ltd.",
  "keywords": [
    "pustaka",
    "api",
    "express",
    "node.js",
    "typescript"
  ],
  "author": "Rohan Rajesh",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
