    "prisma:generate": "prisma generate",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.1",
    "md5": "^2.3.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.7.8",
    "firebase-functions-test": "^3.0.0",
    "prisma": "^4.14.0",
    "typescript": "^4.9.0"
  },
